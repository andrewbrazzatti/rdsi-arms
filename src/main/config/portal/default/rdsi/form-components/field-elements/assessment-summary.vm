<table id="assessment-summary-table" data-bind="stopBindings: true">
    <thead>
        <tr>
        <th>Assessor</th>
        <th>Recommendation</th>
        <th>Size</th>
        <th>Notes</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: rows">
        <tr>
             <td data-bind="text: assessor"/>
             <td data-bind="text: recommendation"/>
             <td data-bind="text: sizeAgreement" />
             <td data-bind="text: comments"/>    
        </tr>
    </tbody>
</table>
